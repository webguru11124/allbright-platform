{
  "expo": {
    "name": "allbright-platform",
    "slug": "allbright-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.allbrightcollective.platform"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "co.allbrightcollective.platform"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/HKGrotesk-Black.otf",
            "./assets/fonts/HKGrotesk-BlackItalic.otf",
            "./assets/fonts/HKGrotesk-BoldItalic.otf",
            "./assets/fonts/HKGrotesk-ExtraBold.otf",
            "./assets/fonts/HKGrotesk-ExtraBoldItalic.otf",
            "./assets/fonts/HKGrotesk-ExtraLight.otf",
            "./assets/fonts/HKGrotesk-ExtraLightItalic.otf",
            "./assets/fonts/HKGrotesk-Italic.otf",
            "./assets/fonts/HKGrotesk-Light.otf",
            "./assets/fonts/HKGrotesk-LightItalic.otf",
            "./assets/fonts/HKGrotesk-Medium.otf",
            "./assets/fonts/HKGrotesk-MediumItalic.otf",
            "./assets/fonts/HKGrotesk-Regular.otf",
            "./assets/fonts/HKGrotesk-SemiBold.otf",
            "./assets/fonts/HKGrotesk-SemiBoldItalic.otf",
            "./assets/fonts/HKGrotesk-Thin.otf",
            "./assets/fonts/HKGrotesk-ThinItalic.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6d4ec78f-e4fc-4ec3-965e-11daf503eee3"
      }
    },
    "owner": "allbright-org"
  }
}